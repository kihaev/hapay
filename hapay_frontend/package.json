{
  "name": "astro-net",
  "version": "0.0.1",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rm -vrf node_modules",
    "clean-dist": "rm -vrf dist",
    "reinstall": "npm run clean && npm i --production",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "server": "node dist/server",
    "build:client-and-server-bundles-devstage": "ng build --prod --configuration=devstage && ng run MasonsTours:server:devstage",
    "build:client-and-server-bundles-production": "ng build --prod && ng run MasonsTours:server:production --bundleDependencies all",
    "build:ssr:devstage": "npm run build:client-and-server-bundles-devstage && npm run compile:server",
    "build:ssr:production": "npm run build:client-and-server-bundles-production && npm run compile:server"
  },
  "private": true,
